 {\n                case 'light':\n                    localStorage.setItem('themePreference', theme); // is this bad practice?\n                    return { ...state, current: theme, theme: lightTheme };\n                case 'dark':\n                    localStorage.setItem('themePreference', theme);\n                    return { ...state, current: theme, theme: darkTheme };\n                case 'dracula':\n                    localStorage.setItem('themePreference', theme);\n                    return { ...state, current: theme, theme: draculaTheme };\n                case 'solarizedDark':\n                    localStorage.setItem('themePreference', theme);\n                    return {\n                        ...state,\n                        current: theme,\n                        theme: solarizedDarkTheme,\n                    };\n                case 'solarizedLight':\n                    localStorage.setItem('themePreference', theme);\n                    return {\n                        ...state,\n                        current: theme,\n                        theme: solarizedLightTheme,\n                    };\n                case 'gruvboxDark':\n                    localStorage.setItem('themePreference', theme);\n                    return { ...state, current: theme, theme: gruvboxDarkTheme };\n                case 'nord':\n                    localStorage.setItem('themePreference', theme);\n                    return { ...state, current: theme, theme: nordTheme };\n                default:\n                    return state;\n            }\n        },\n    },\n});\n\nexport const { changeTheme } = themeSlice.actions;\nexport const themeState = (state: any) => state.theme;\nexport default themeSlice.reducer;\n","/home/sanaz/remember-tailwind/src/reducers/auth.js",["185","186"],"/home/sanaz/remember-tailwind/src/reducers/index.js",[],"/home/sanaz/remember-tailwind/src/styles/themes.js",[],"/home/sanaz/remember-tailwind/src/utils/index.js",[],"/home/sa