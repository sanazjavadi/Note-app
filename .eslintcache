rr;\n        });\n    return response;\n});\n\nexport const DeleteNote = createAsyncThunk('delete/note', (NoteId) => {\n    const response = instance\n        .delete(`/v1/user/${parsedUser.id}/note/${NoteId}`)\n        .then((res) => {\n            console.log(res);\n            return res.data.data;\n        })\n        .catch((err) => {\n            console.log(err.response);\n            return err.response || err;\n        });\n    return response;\n});\n\nconst notesSlice = createSlice({\n    name: 'notes',\n    initialState: {},\n    extraReducers: {\n        [getNotes.fulfilled]: (state, action) => {\n            return action.payload;\n        },\n        [createNote.fulfilled]: (state, action) => {\n            return [...state, action.payload];\n        },\n    },\n});\n\nexport const notesState = (state) => state.notes;\nexport default notesSlice.reducer;\n","/home/sanaz/remember-tailwind/src/components/Note/index.tsx",[],"/home/sanaz/remember-tailwind/src/components/EditNoteBook/index.tsx",[],{"ruleId":"198","replacedBy":"199"},{"ruleId":"200","replacedBy":"201"},{"ruleId":"202","replacedBy":"203"},{"ruleId":"204","replacedBy":"205"},{"ruleId":"206","replacedBy":"207"},{"ruleId":"208","replacedBy":"209"},{"ruleId":"210","replacedBy":"211"},{"ruleId":"198","replacedBy":"199"},{"ruleId":"200","replacedBy":"212"},{"ruleId":"202","replacedBy":"203"},{"ruleId":"204","replacedBy":"205"},{"ruleId":"206","replacedBy":"207"},{"ruleId":"208","replacedBy":"209"},{"ruleId":"210","replacedBy":"211"},{"ruleId":"213","severity":1,"message":"214","line":74,"column":27,"nodeType":"215","messageId":"216","endLine":74,"endColumn":42},{"ruleId":"213","severity":1,"message":"217","line":74,"column":28,"nodeType":"218","messageId":"219","endLine":74,"endColumn":38},{"ruleId":"220","severity":1,"message":"221","line":74,"column":35,"nodeType":"222","messageId":"223","endLine":74,"endColumn":38,"suggestions":"224"},{"ruleId":"213","severity":1,"message":"214","line":78,"column