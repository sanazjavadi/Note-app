ata)\n        .then((res) => {\n            return res.data.data;\n        })\n        .catch((err) => {\n            return err.response || err;\n        });\n    return response;\n});\n\nexport const DeleteNote = createAsyncThunk('delete/note', (NoteId) => {\n    const response = instance\n        .delete(`/v1/user/${parsedUser.id}/note/${NoteId}`)\n        .then((res) => {\n            return res.data.data;\n        })\n        .catch((err) => {\n            return err.response || err;\n        });\n    return response;\n});\n\nconst notesSlice = createSlice({\n    name: 'notes',\n    initialState: {\n        notes: [],\n        loading: false,\n    },\n    extraReducers: {\n        [getNotes.pending]: (state) => {\n            return { ...state, loading: true };\n        },\n        [getNotes.fulfilled]: (state, action) => {\n            return { ...state, loading: false, notes: action.payload };\n        },\n        [createNote.fulfilled]: (state, action) => {\n            return { ...state, loading: false, notes: [...state.notes, action.payload] };\n        },\n    },\n});\n\nexport const notesState = (state) => state.notes;\nexport default notesSlice.reducer;\n","/home/sanaz/remember-tailwind/src/components/Note/index.tsx",[],"/home/sanaz/remember-tailwind/src/components/EditNoteBook/index.tsx",[],"/home/sanaz/remember-tailwind/src/svg/Loading.js",[],"/home/sanaz/remember-tailwind/src/utils/validateInfo.js",["207","208"],{"ruleId":"209","replacedBy":"210"},{"ruleId":"211","replacedBy":"212"},{"ruleId":"213","replacedBy":"214"},{"ruleId":"215","replacedBy":"216"},{"ruleId":"217","replacedBy":"218"},{"ruleId":"219","replacedBy":"220"},{"ruleId":"221","replacedBy":"222"},{"ruleId":"209","replacedBy":"210"},{"ruleId":"211","replacedBy":"223"},{"ruleId":"213","replacedBy":"214"},{"ruleId":"215","replacedBy":"216"},{"ruleId":"217","replacedBy":"218"},{"ruleId":"219","replacedBy":"220"},{"ruleId":"221","replacedBy":"222"},{"ruleId":"224","severity":1,"message":"225"